{
    "name": "renoki-co/hej",
    "description": "Hej! is a simple authentication boilerplate for Socialite.",
    "keywords": ["laravel", "php", "social", "login", "register", "socialite", "facebook", "github", "twitter", "gitlab", "google"],
    "license": "MIT",
    "homepage": "https://github.com/renoki-co/hej",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": "^6.0|^7.0|^8.0",
        "laravel/socialite": "^4.4|^5.0"
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\Hej\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\Hej\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.0.4",
        "orchestra/database": "^4.0|^5.0|^6.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "orchestra/testbench-browser-kit": "^4.0|^5.0|^6.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "RenokiCo\\Hej\\HejServiceProvider"
            ]
        }
    }
}
